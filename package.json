{
  "name": "eventmonkey",
  "version": "0.0.1",
  "description": "Discord package to add event scheduling to bots",
  "main": "dist/EventMonkey.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --exec \"npm run build\"",
    "build": "tsc && copy-files-from-to"
  },
  "copyFiles": [
    {
      "from": "package.dist.json",
      "to": "dist/package.json"
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.8.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5"
  }
}
